<%= form_for @user do |f| %>
<%= f.text_field :username, placeholder: "Username" %>
<%= f.password_field :password, placeholder: "Password" %>
<%= f.password_field :password_confirmation, placeholder: "Password Confirmation" %>
<%= f.text_field :first_name, placeholder: "Enter your First Name" %>
<%= f.text_field :last_name, placeholder: "Enter your Last Name" %>
<%= f.text_field :email, placeholder: "Enter your Email" %>
<%= f.date_select :birth_date, start_year: 1900 %>
<%= f.text_field :address, placeholder: "Address" %>
<%= f.text_field :address2, placeholder: "Address 2" %>
<%= f.text_field :city, placeholder: "City" %>
<%= f.text_field :state, placeholder: "State" %>
<%= f.number_field :postal_code, placeholder: "Postal Code" %>
<%= f.submit "Create User" %>
<% end %>


